{
    "name": "mcp_orchestrator",
    "version": "1.0.0",
    "main": "dist/server.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/server.js",
        "start:http": "node dist/hybrid-server.js http",
        "start:mcp": "node dist/warp-mcp.js",
        "start:hybrid": "node dist/hybrid-server.js hybrid",
        "dev": "ts-node src/server.ts",
        "dev:mcp": "ts-node src/warp-mcp.ts",
        "dev:hybrid": "ts-node src/hybrid-server.ts hybrid",
        "dev:watch": "nodemon --exec ts-node src/index.ts",
        "clean": "rimraf dist"
    },
    "dependencies": {
        "express": "^5.1.0",
        "pg": "^8.16.3",
        "@modelcontextprotocol/sdk": "1.17.4"
    },
    "devDependencies": {
        "@types/express": "^5.0.3",
        "@types/node": "24.3.0",
        "@types/pg": "^8.15.5",
        "nodemon": "3.1.10",
        "rimraf": "6.0.1",
        "ts-node": "10.9.2",
        "typescript": "5.9.2"
    }
}
